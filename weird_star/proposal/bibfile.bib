
@article{hartmann_nature_1985,
	title = {On the nature of {FU} {Orionis} objects},
	volume = {299},
	issn = {0004-637X},
	url = {http://adsabs.harvard.edu/abs/1985ApJ...299..462H},
	doi = {10.1086/163713},
	abstract = {We argue that rapid accretion onto pre-main sequence stars causes the outbursts of FU Orionis objects. In our model, emission from the hot, optically thick accretion disk dominates the system light at maximum. The radial temperature gradient of the disk produces an M-type spectrum in the infrared, a G-type spectrum at optical wavelengths, and an excess below 4000 Å, as observed. The absence of detectable radial
velocity variations leads us to suggest that the accreted material comes from a surrounding protostellar nebula, rather than a companion star.  A crucial prediction of the disk model is that weak absorption lines should be double-peaked. We report high-resolution spectra of V1057 Cyg which show double-peaked profiles in red photospheric lines. Herbig and Petrov have observed a similar effect in FU On, indicating that doubled profiles are characteristic of FU Orionis objects. In principle, the double absorption might be produced by a spectroscopic binary system, but the orbital period of such a system must be short, and no evidence for velocity-crossing has yet been observed.  We further propose that the observed massive winds of FU Orionis objects arise from the luminous disk surface. The large occultation of redshifted material implied by this picture is in agreement with the meager redshifted emission observed in the Hα P Cygni profiles of these objects. Bipolar
outflows have been associated with a number of pre-main sequence stars; we point out that ejection from a disk can naturally and efficiently produce winds with the appropriate geometry.},
	urldate = {2017-02-07},
	journal = {The Astrophysical Journal},
	author = {Hartmann, L. and Kenyon, S. J.},
	month = dec,
	year = {1985},
	keywords = {Accretion Disks, Binary Stars, High Resolution, Line Spectra, Pre-Main Sequence Stars, Radial Velocity, Stellar Color, Stellar Mass Accretion, Stellar Mass Ejection, Stellar Spectrophotometry, Variable Stars},
	pages = {462--478},
	file = {NASA/ADS Full Text PDF:/home/fareij/.zotero/zotero/tcnivcww.default/zotero/storage/A6QPVTSI/Hartmann and Kenyon - 1985 - On the nature of FU Orionis objects.pdf:application/pdf}
}

@article{herbig_eruptive_1977,
	title = {Eruptive phenomena in early stellar evolution},
	volume = {217},
	issn = {0004-637X},
	url = {http://adsabs.harvard.edu/abs/1977ApJ...217..693H},
	doi = {10.1086/155615},
	abstract = {Available optical material on FU Ori, V1057 Cyg, and V1515 Cyg is reviewed, and some tentative ideas are offered concerning the nature and frequency of the FU Ori phenomenon. The more recent photometric behavior of FU Ori and evidence for its association with interstellar matter are described, with attention given to the star's IR excess, photographic/B light curve, and radial velocities. The early history of V1057 is examined along with its spectral classification, radial velocities, line structure, colors, dimensions, IR excesses, and associated reflection nebulae. The discovery of V1515 Cyg is discussed, spectral
characteristics and statistics of the three known FU Ori variables are evaluated, and it is suggested that the mean time between successive outbursts of the FU Ori type in an individual T Tauri star is roughly 10,000 years. Some effects of one or more FU Ori-type eruptions in the young sun on matter in the inner solar nebula are considered together with several proposed explanations for the cause of such outbursts. It is concluded that no convincing explanation of the FU Ori phenomenon is as yet available.},
	urldate = {2017-02-07},
	journal = {The Astrophysical Journal},
	author = {Herbig, G. H.},
	month = nov,
	year = {1977},
	keywords = {Early Stars, Emission Spectra, Infrared Spectra, Line Spectra, Main Sequence Stars, Metallic Stars, Nebulae, Radial Velocity, Spectrograms, Stellar Atmospheres, Stellar Evolution, Stellar Mass Ejection, Stellar Rotation, Stellar Spectra, Stellar Spectrophotometry, Supergiant Stars, T Tauri Stars, Variable Stars},
	pages = {693--715},
	file = {NASA/ADS Full Text PDF:/home/fareij/.zotero/zotero/tcnivcww.default/zotero/storage/B65S56XZ/Herbig - 1977 - Eruptive phenomena in early stellar evolution.pdf:application/pdf}
}

@article{kenyon_flickering_2000,
	title = {Flickering in {FU} {Orionis}},
	volume = {531},
	issn = {0004-637X},
	url = {http://stacks.iop.org/0004-637X/531/i=2/a=1028},
	doi = {10.1086/308515},
	abstract = {We analyze new and published optical photometric data of FU Orionis, an eruptive pre-main-sequence star. The outburst consists of a 5.5 mag rise at B with an e -folding timescale of 50 days. The rates of decline at B and V are identical, 0.015 ± 0.001 mag yr -1 . Random fluctuations superimposed on this decline have an amplitude of 0.035 ± 0.005 mag at V and occur on timescales of 1 day or less. Correlations between V and the color indices U - B , B - V , and V - R indicate that the variable source has the optical colors of a G0 supergiant. We associate this behavior with small-amplitude flickering of the inner accretion disk.},
	language = {en},
	number = {2},
	urldate = {2017-02-07},
	journal = {The Astrophysical Journal},
	author = {Kenyon, Scott J. and Kolotilov, E. A. and Ibragimov, M. A. and Mattei, Janet A.},
	year = {2000},
	pages = {1028},
	file = {IOP Full Text PDF:/home/fareij/.zotero/zotero/tcnivcww.default/zotero/storage/C93QNZKJ/Kenyon et al. - 2000 - Flickering in FU Orionis.pdf:application/pdf}
}

@incollection{kenyon_accretion_1999,
	series = {{NATO} {Science} {Series}},
	title = {Accretion {Disks} and {Eruptive} {Phenomena}},
	copyright = {©1999 Springer Science+Business Media Dordrecht},
	isbn = {978-0-7923-5909-8 978-94-011-4509-1},
	url = {http://link.springer.com/chapter/10.1007/978-94-011-4509-1_18},
	abstract = {In the 1700’s, Immanuel Kant and the Marquis de Laplace proposed that the solar system collapsed from a gaseous medium of roughly uniform density ([79], [105]). A flattened gaseous disk — the protosolar nebula — formed out of this cloud. The Sun contracted out of material at the center of the disk, while the planets condensed in the outer portions. Despite its simplicity, this model suffered from the angular momentum problem inherent to star formation: a cloud of gas and dust with the diameter of the solar system and the mass of the Sun has too much angular momentum to collapse to the Sun’s present size. It took 150 years to solve this problem. C. von Weiszäcker worked out the basic physics of a viscous accretion disk, building on previous realizations that a turbulent viscosity could move material inwards and angular momentum outwards through the protosolar nebula ([178], [179]). With Lüst’s steady-state solution ([119]), Kant’s nebular hypothesis no longer suffered from the angular momentum problem and became the leading model for solar system formation.},
	language = {en},
	number = {540},
	urldate = {2017-02-07},
	booktitle = {The {Origin} of {Stars} and {Planetary} {Systems}},
	publisher = {Springer Netherlands},
	author = {Kenyon, Scott J.},
	editor = {Lada, Charles J. and Kylafis, Nikolaos D.},
	year = {1999},
	note = {DOI: 10.1007/978-94-011-4509-1\_18},
	keywords = {Astronomy, Observations and Techniques, Astrophysics and Astroparticles},
	pages = {613--642},
	file = {Kenyon1999 Accretion Disks and Eruptive Phenomena.pdf:/home/fareij/.zotero/zotero/tcnivcww.default/zotero/storage/4VAQAXC8/Kenyon1999 Accretion Disks and Eruptive Phenomena.pdf:application/pdf;Snapshot:/home/fareij/.zotero/zotero/tcnivcww.default/zotero/storage/43SXXISD/978-94-011-4509-1_18.html:text/html}
}

@article{turner_models_1997,
	title = {Models of the {Spectral} {Energy} {Distributions} of {FU} {Orionis} {Stars}},
	volume = {480},
	issn = {0004-637X},
	url = {http://stacks.iop.org/0004-637X/480/i=2/a=754},
	doi = {10.1086/303983},
	abstract = {Observed spectral energy distributions (SEDs) of FU Orionis, V1057 Cygni, and V1515 Cygni are fitted by theoretical spectra, which are calculated from models consisting of outbursting accretion disks together with flattened envelopes. Temperature in the envelopes is determined by approximate radiative equilibrium with a central source. The disk models are two-dimensional and include reprocessing of disk radiation by the disk. The theoretical spectra are calculated using a radiative transfer code and frequency-dependent opacities, at a spectral resolution of λ/Δλ = 14. Excellent matches to the data are obtained for all three objects with reasonable model parameters. Radiative transfer is also used to calculate a time series of images showing the progress of an outburst as imaged through a B -band filter.},
	language = {en},
	number = {2},
	urldate = {2017-02-07},
	journal = {The Astrophysical Journal},
	author = {Turner, N. J. J. and Bodenheimer, P. and Bell, K. R.},
	year = {1997},
	pages = {754},
	file = {IOP Full Text PDF:/home/fareij/.zotero/zotero/tcnivcww.default/zotero/storage/8AJ5INN2/Turner et al. - 1997 - Models of the Spectral Energy Distributions of FU .pdf:application/pdf}
}

@article{petrov_interpretation_1992,
	title = {On the interpretation of the spectrum of {FU} {Orionis}},
	volume = {392},
	issn = {0004-637X},
	url = {http://adsabs.harvard.edu/abs/1992ApJ...392..209P},
	doi = {10.1086/171419},
	abstract = {The paper addresses several difficulties with the accretion disk interpretation of the spectra of FU Orionis objects. It is argued that the double absorption lines observed in FU Ori and other members of the group, which have been claimed to be the signature of a self-luminous Keplerian disk, can be produced equally well by a single rotating star. The properties of the absorption-line spectrum of FU Ori as observed at a resolution of about 50,000 are described and compared to simple models of a single rotator with a stratified atmosphere. It is shown that the line structure of FU Ori can be reproduced by a model consisting of an emission-line shell overlaid by a cooler absorbing layer, the two superimposed upon a macroturbulent G-type supergiant spectrum. In the present model, the appearance of line doubling is due to the appearance of weak emission cores in the metallic absorption lines, while
curve-of-growth effects account for the observed dependence of reversal intensity upon absorption line strength.},
	urldate = {2017-02-07},
	journal = {The Astrophysical Journal},
	author = {Petrov, P. P. and Herbig, G. H.},
	month = jun,
	year = {1992},
	keywords = {Absorption Spectra, Emission Spectra, Line Spectra, Orion Constellation, Spectrum Analysis, Stellar Luminosity, Stellar Models, Stellar Spectra, Stellar Structure},
	pages = {209--217},
	file = {NASA/ADS Full Text PDF:/home/fareij/.zotero/zotero/tcnivcww.default/zotero/storage/46KHH7SF/Petrov and Herbig - 1992 - On the interpretation of the spectrum of FU Orioni.pdf:application/pdf}
}

